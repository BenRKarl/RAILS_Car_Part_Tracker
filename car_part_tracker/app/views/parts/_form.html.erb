<%= form_for [@car, @part] do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name %></br>
  <%= f.label :part_type %>:
  <%= f.text_field :part_type %></br>
  <%= f.label :part_number %>:
  <%= f.text_field :part_number %></br>
  <%= f.label :repair_details %>:
  <%= f.text_field :repair_details %></br>
  <%= f.submit %>
<% end %>
