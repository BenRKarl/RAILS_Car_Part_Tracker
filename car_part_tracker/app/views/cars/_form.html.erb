<%= form_for @car, :url => cars_path(@car) do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name %></br>
  <%= f.label :VIN %>:
  <%= f.text_field :VIN %></br>
  <%= f.label :make %>:
  <%= f.text_field :make %></br>
  <%= f.label :model %>:
  <%= f.text_field :model %></br>
  <%= f.submit 'Create New Car' %>
<% end %>


